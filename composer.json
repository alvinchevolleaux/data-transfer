{
    "name": "alvinchevolleaux/data-transfer",
    "description": "Simple API data parsing functionality with open standardsd",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alvin Chevolleaux",
            "email": "alvin@chevolleaux.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["bin/schema"],
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "json-schema/json-schema-test-suite": "2.0.0"
    },
    "autoload": {
        "psr-4": {
            "DataTransfer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DataTransferTest\\": "test/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "json-schema/json-schema-test-suite",
                "version": "2.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/json-schema/JSON-Schema-Test-Suite",
                    "reference": "2.0.0"
                }
            }
        }
    ]
}
